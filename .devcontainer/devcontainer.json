{
    "name": "stations_optim",
    "build": {
      "dockerfile": "../Dockerfile"
    },
    "workspaceFolder": "/home",
    "remoteUser": "root",
    "postCreateCommand": "pip install --upgrade pip && pip install jupyterlab ipykernel",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ],
        "settings": {
          "terminal.integrated.defaultProfile.linux": "bash"
        }
      }
    },
    "mounts": [
      "source=${localWorkspaceFolder},target=/home,type=bind"
    ]
  }
  